{
  "name": "opentelemetry-instrumentation-commander-monorepo",
  "version": "1.0.0",
  "description": "OpenTelemetry instrumentation for Commander.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w packages/opentelemetry-instrumentation-commander && npm run build -w packages/example-cli",
    "test": "npm run test -w packages/opentelemetry-instrumentation-commander",
    "clean": "rm -rf packages/*/dist packages/*/node_modules node_modules",
    "pack:instrumentation": "npm pack -w packages/opentelemetry-instrumentation-commander",
    "publish:instrumentation": "npm publish -w packages/opentelemetry-instrumentation-commander --access public"
  },
  "keywords": [
    "opentelemetry",
    "instrumentation",
    "commander",
    "cli"
  ],
  "author": "k65miyazakiy",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
